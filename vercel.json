{
    "github": {
        "silent": true
    },
    "buildCommand": "cd uni-app && npm install && npm run build",
    "outputDirectory": "uni-app/dist",
    "rewrites": [
        {
            "source": "/assets/(.*)",
            "destination": "/assets/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}